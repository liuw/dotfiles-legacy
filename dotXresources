URxvt.perl-ext-common: default,matcher,keyboard-select,selection-to-clipboard,clipboard,font-size

!,tabbedex

URxvt.keysym.M-Escape: perl:keyboard-select:activate

! Do nott show scroll bar
URxvt.scrollBar: off
! Do not scroll output
URxvt.scrollTtyOutput: false
! Scroll in relation to buffer (with mouse scroll or Shift+PgUp)
URxvt.scrollWithBuffer: true
! Scroll to bottom on keypress
URxvt.scrollTtyKeypress: true
! Disalbe scroll back buffer for secondary screen
URxvt.secondaryScreen: true
URxvt.secondaryScroll: false

URxvt*urgentOnBell: false
URxvt*shading:200

! Comment out following line to use bitmap font
! URxvt*font: xft:Monospace:size=10
! URxvt*font: xft:Bitstream Vera Sans Mono:size=9
! URxvt*font: xft:Bitstream Vera Sans Mono:size=10:hintstyle=none
! URxvt*font: xft:Liberation Mono:size=10
! URxvt*font: xft:DejaVu Sans Mono:size=9
! URxvt*font: xft:PT Mono:size=10
! URxvt*font: xft:Monte Carlo:size=9:antialias=false
! URxvt*font: xft:Source Code Pro:size=9

! No bold font please
URxvt*boldFont:


! URxvt.letterSpace: -1


URxvt.loginShell: true
URxvt.visualBell: false

! Clickable URLs, requires matcher
URxvt.matcher.button: 1
! URxvt.urlLauncher: /usr/bin/xdg-open
URxvt.url-launcher: /usr/bin/xdg-open

! Copy and paste, requires clippboard module and xclip
! See ~/.urxvt/ext/clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: false


